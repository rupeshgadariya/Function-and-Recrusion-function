// Write a function to swap strings of two char arrays. 
 #include<stdio.h>
 
 void swap( char *str, char *ch, int num )
 {
    char temp;

    for (int i = 0; num>i; i++)
    {
        temp = str[i];
        str[i] = ch[i];
        ch[i] = temp;

    }
    
    
 }

    int main()
 {
        char str[20]="hello";
        char ch[20]= "world";
       printf("%s\n%s\n",str,ch);
        swap(str,ch,20);
       printf("%s\n%s",str,ch);
      
    return 0;
 }
