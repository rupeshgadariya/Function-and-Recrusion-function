// Write a function to convert a given string into uppercase. 
#include<stdio.h>
#include<string.h>

    void upp(char *str)
    {
        for (int i = 0; str[i]; i++)
          if (str[i]>='a' && str[i]<='z')
            str[i] = str[i]-32;
          
    }
        

    int main()
{
    char sr[30];
    fgets(sr,30,stdin);
  
    puts(sr);
    upp(sr);
    puts(sr);       

        
    return 0;
}
