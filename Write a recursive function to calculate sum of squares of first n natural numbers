// Write a recursive function to calculate sum of squares of first n natural numbers 
#include<stdio.h>
#include<conio.h>
int square_sum(int num)
{
    if (num==1)   return 1;

   return num*num + square_sum(num-1);
    
}

    void main()
{
        printf("%d", square_sum(15));
        
    getch();
}
