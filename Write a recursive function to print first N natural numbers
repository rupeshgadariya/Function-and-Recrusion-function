// Write a recursive function to print first N natural numbers 

#include<stdio.h>
#include<conio.h>

    void natural( int n)
    {       
        if(n==0)
        return;
        
        natural(n-1); 
        printf("%d  ",n);
    }

        void main()
    {   
        void natural( int n);
        natural(10);
            
            
        getch();
    }
